    <!-- http://purl.obolibrary.org/obo/LifO_0000007 -->

    <owl:ObjectProperty rdf:about="&obo;LifO_0000007">
        <rdfs:label xml:lang="en">has life blueprint</rdfs:label>
        <obo:IAO_0000115>An object property that represents a relation between a life process and its genetic design stored in the genotype of an organism</obo:IAO_0000115>
    </owl:ObjectProperty>
    


